<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Member">
	<select id="getMemberById" resultType="memberdto">
		SELECT * FROM MEMBER
		WHERE USER_ID = #{user_id}
	</select>

</mapper>